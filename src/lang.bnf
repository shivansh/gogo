IdentifierList : identifier
                        << func() (Attrib, error) {
                                return Node{"", []string{string($0.(*token.Token).Lit)}}, nil
                        } () >>
               | identifier "," IdentifierList
                        << func() (Attrib, error) {
                                n := Node{"", []string{string($0.(*token.Token).Lit)}}
                                n.code = append(n.code, $2.(Node).code...)
                                return n, nil
                        } () >>
