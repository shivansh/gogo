CompositeLit : LiteralType LiteralValue
                        << func() (Attrib, error) {
                                n := Node{"", []string{}}
                                // For a struct, LiteralType stores its name.
                                symTabEntry, found := GetSymTabEntry($0.(Node).place)
                                if found {
                                        switch symTabEntry[0] {
                                        case "struct":
                                                var varName, varType string
                                                for k, v := range symTabEntry[1:] {
                                                        if k % 2 == 0 {
                                                                // variable names are present at even locations.
                                                                varName = v
                                                        } else {
                                                                // variable types are present at odd locations.
                                                                varType = v
                                                                switch varType {
                                                                case "int":
                                                                        // add a symbol table entry corresponding
                                                                        // to the struct member.
                                                                        // The symbol table entry key is of the form:
                                                                        //      structName.structMember
                                                                        renamedVar := NewTmp()
                                                                        currSymTab.varSymTab[fmt.Sprintf("%s.%s", $0.(Node).place, varName)] = []string{renamedVar, "int"}
                                                                        n.code = append(n.code, fmt.Sprintf("=, %s, 0", renamedVar))
                                                                default: // TODO: Handle different types
                                                                        return nil, errors.New("Unknown type")
                                                                }
                                                        }
                                                }
                                        }
                                } else {
                                        return nil, errors.New("struct not in scope")
                                }
                                return n, nil
                        } () >>
             ;

